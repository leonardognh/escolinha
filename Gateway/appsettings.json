{
  "ReverseProxy": {
    "Routes": {
      "professores-api": {
        "ClusterId": "professores-api",
        "Match": {
          "Path": "/api/professores/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/api/professores" }
        ]
      },
      "professores-docs": {
        "ClusterId": "professores-api",
        "Match": {
          "Path": "/scalar/professores/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/professores" }
        ]
      },

      "alunos-api": {
        "ClusterId": "alunos-api",
        "Match": {
          "Path": "/api/alunos/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/api/alunos" }
        ]
      },
      "alunos-docs": {
        "ClusterId": "alunos-api",
        "Match": {
          "Path": "/scalar/alunos/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/alunos" }
        ]
      },

      "grade-api": {
        "ClusterId": "grade-api",
        "Match": {
          "Path": "/api/grade/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/api/grade" }
        ]
      },
      "grade-docs": {
        "ClusterId": "grade-api",
        "Match": {
          "Path": "/scalar/grade/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/grade" }
        ]
      },

      "turmas-api": {
        "ClusterId": "turmas-api",
        "Match": {
          "Path": "/api/turmas/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/api/turmas" }
        ]
      },
      "turmas-docs": {
        "ClusterId": "turmas-api",
        "Match": {
          "Path": "/scalar/turmas/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/turmas" }
        ]
      },

      "materias-api": {
        "ClusterId": "materias-api",
        "Match": {
          "Path": "/api/materias/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/api/materias" }
        ]
      },
      "materias-docs": {
        "ClusterId": "materias-api",
        "Match": {
          "Path": "/scalar/materias/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/materias" }
        ]
      }
    },
    "Clusters": {
      "professores-api": {
        "Destinations": {
          "default": { "Address": "http://professores:80/" }
        }
      },
      "alunos-api": {
        "Destinations": {
          "default": { "Address": "http://alunos:80/" }
        }
      },
      "grade-api": {
        "Destinations": {
          "default": { "Address": "http://grade:80/" }
        }
      },
      "turmas-api": {
        "Destinations": {
          "default": { "Address": "http://turmas:80/" }
        }
      },
      "materias-api": {
        "Destinations": {
          "default": { "Address": "http://materias:80/" }
        }
      }
    }
  }
}
