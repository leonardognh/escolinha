{
  "ReverseProxy": {
    "Routes": [
      {
        "RouteId": "professores",
        "ClusterId": "professores-api",
        "Match": {
          "Path": "/api/professores/{**catch-all}"
        },
        "Transforms": [ { "PathRemovePrefix": "/api/professores" } ]
      },
      {
        "RouteId": "alunos",
        "ClusterId": "alunos-api",
        "Match": {
          "Path": "/api/alunos/{**catch-all}"
        },
        "Transforms": [ { "PathRemovePrefix": "/api/alunos" } ]
      },
      {
        "RouteId": "grade",
        "ClusterId": "grade-api",
        "Match": {
          "Path": "/api/grade/{**catch-all}"
        },
        "Transforms": [ { "PathRemovePrefix": "/api/grade" } ]
      },
      {
        "RouteId": "turmas",
        "ClusterId": "turmas-api",
        "Match": {
          "Path": "/api/turmas/{**catch-all}"
        },
        "Transforms": [ { "PathRemovePrefix": "/api/turmas" } ]
      },
      {
        "RouteId": "materias",
        "ClusterId": "materias-api",
        "Match": {
          "Path": "/api/materias/{**catch-all}"
        },
        "Transforms": [ { "PathRemovePrefix": "/api/materias" } ]
      }
    ],
    "Clusters": {
      "professores-api": {
        "Destinations": {
          "default": { "Address": "http://localhost:5001/" }
        }
      },
      "alunos-api": {
        "Destinations": {
          "default": { "Address": "http://localhost:5003/" }
        }
      },
      "grade-api": {
        "Destinations": {
          "default": { "Address": "http://localhost:5005/" }
        }
      },
      "turma-api": {
        "Destinations": {
          "default": { "Address": "http://localhost:5007/" }
        }
      },
      "materia-api": {
        "Destinations": {
          "default": { "Address": "http://localhost:5009/" }
        }
      }
    }
  }
}
